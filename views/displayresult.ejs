<html><body>
  <head>
    <meta charset="UTF-8" />
    <title>MyAnswerData</title>
  </head>
  <table border="1" cellpadding="7" cellspacing="7">
    <tr>
      <th width="50px">No</th>
      <th>Питання</th>
      <th>Відповідь</th>
    </tr>
    <% if(data.length){ 
      for(var i = 0;i < data.length;i++) { %>
    <tr>
      <td><%=(i+1)%></td>
      <td>
        <%
        for(var j = 0;j < splited_content[i].length;j++) { %>
          <%=splited_content[i][j]%> <br>
        <% }
        %>
      </td>
      <td><%=data[i]%></td>
    </tr>
    <% }
    }else{ %>
    <tr>
      <td colspan="3">No data</td>
    </tr>
  <% } %>
  </table>
  </body></html>